{
  "name": "notesview",
  "version": "1.0.0",
  "description": "A simple piano sight-reading trainer built with React and VexFlow",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md}\""
  },
  "keywords": [
    "piano",
    "music",
    "sight-reading",
    "react",
    "vexflow",
    "tone.js"
  ],
  "author": "AbassWithoutBass",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tone": "^15.1.22",
    "vexflow": "^5.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.6.2",
    "vite": "^7.1.12"
  }
}
